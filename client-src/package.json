{
  "name": "logistics-wizard",
  "version": "0.1.0",
  "description": "Front end UI for Logistics Wizard demo",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "test:dev-verbose": "npm run test -- --verbose --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "storybook": "start-storybook -p 9001",
    "codecov": "cat coverage/lcov.info | codecov",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node_modules/.bin/nyc node --harmony-proxies ./node_modules/.bin/ava",
      "env": {
        "NODE_ENV": "test",
        "NODE_PATH": "src",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM-Bluemix/logistics-wizard.git"
  },
  "author": "Colby Williams <colbycheeze@us.ibm.com> (http://colbycheeze.com)",
  "license": "MIT",
  "dependencies": {
    " rimraf": "^2.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.10",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-plugin-import": "^1.12.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "fs-extra": "^0.30.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.6.0",
    "koa-static": "^3.0.0",
    "material-ui": "^0.15.2",
    "node-sass": "^3.7.0",
    "nodemon": "^1.10.0",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.9.0",
    "react": "^15.3.0",
    "react-container-query": "^0.5.4",
    "react-dom": "^15.3.0",
    "react-fa": "^4.1.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.0",
    "redux-saga": "^0.11.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-hot-middleware": "^2.12.2",
    "yargs": "^4.0.0"
  },
  "devDependencies": {
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "^2.1.0",
    "ava": "^0.15.2",
    "babel-eslint": "^6.0.0-beta.6",
    "enzyme": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mock-css-modules": "^1.0.0",
    "nock": "^8.0.0",
    "nodemon": "^1.8.1",
    "nyc": "^7.1.0",
    "opn": "^4.0.2",
    "react-addons-test-utils": "^15.3.0",
    "redbox-react": "^1.2.10",
    "redux-ava": "^2.1.0",
    "sinon": "^1.17.3",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "require": [
      "babel-register",
      "./config/ava.config.js"
    ],
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.js",
      "!src/**/*.story.js"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.test.js",
      "src/**/*.story.js",
      "src/routes/**/index.js",
      "src/store/*"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
